    iao:IAO_0000115 "A digital health method using smartphone applications to collect, track, or analyze physical activity or health data. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

pafh:TelehealthAssessmentMethod a owl:Class ;
    rdfs:label "Telehealth Assessment Method" ;
    rdfs:subClassOf pafh:DigitalHealthMethod ;
    iao:IAO_0000115 "A digital health method using telehealth platforms to remotely assess physical activity, fitness, or health outcomes. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

pafh:OnlineSurveyMethod a owl:Class ;
    rdfs:label "Online Survey Method" ;
