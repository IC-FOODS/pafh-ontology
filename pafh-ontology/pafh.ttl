pafh:EarHealth a owl:Class ;
    rdfs:label "Ear Health" ;
    rdfs:subClassOf pafh:SensorySystemHealth ;
    iao:IAO_0000115 "A disposition of the ears to enable hearing and balance, realized by the function of cochlea, vestibular apparatus, and auditory nerves. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:TongueHealth a owl:Class ;
    rdfs:label "Tongue Health" ;
    rdfs:subClassOf pafh:SensorySystemHealth ;
