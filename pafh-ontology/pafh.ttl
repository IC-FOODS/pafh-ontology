pafh:NoseHealth a owl:Class ;
    rdfs:label "Nose Health" ;
    rdfs:subClassOf pafh:SensorySystemHealth ;
    iao:IAO_0000115 "A disposition of the nose to enable smell and filter inhaled air, realized by the function of olfactory epithelium and nasal passages. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:MentalHealth a owl:Class ;
