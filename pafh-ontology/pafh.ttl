# --- PHYSICAL FITNESS BRANCH ---

pafh:PhysicalFitness a owl:Class ;
    rdfs:label "Physical Fitness" ;
    rdfs:subClassOf bfo:Disposition ;
    iao:IAO_0000115 "A set of attributes that an individual has or achieves relating to the ability to perform physical activity, encompassing health-related and skill-related dimensions. [ACSM 2018, CDC 2023, ICF d570]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d570> ;
    skos:closeMatch <http://purl.bioontology.org/ontology/SNOMEDCT/630403009> ;
    skos:closeMatch <http://purl.obolibrary.org/obo/OBI_0000245> .

pafh:HealthRelatedFitness a owl:Class ;
    rdfs:label "Health-Related Fitness" ;
    rdfs:subClassOf pafh:PhysicalFitness ;
    iao:IAO_0000115 "A dimension of physical fitness related to health outcomes and the ability to perform daily activities. [CDC 2023, ACSM 2018]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d570> .

pafh:CardiorespiratoryEndurance a owl:Class ;
    rdfs:label "Cardiorespiratory Endurance" ;
    rdfs:subClassOf pafh:HealthRelatedFitness ;
    iao:IAO_0000115 "The ability of the circulatory and respiratory systems to supply oxygen during sustained physical activity. [ACSM 2018, CDC 2023]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d455> .

pafh:MuscularStrength a owl:Class ;
    rdfs:label "Muscular Strength" ;
    rdfs:subClassOf pafh:HealthRelatedFitness ;
    iao:IAO_0000115 "The ability of muscle to exert force. [ACSM 2018, CDC 2023]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d430> .

pafh:MuscularEndurance a owl:Class ;
    rdfs:label "Muscular Endurance" ;
    rdfs:subClassOf pafh:HealthRelatedFitness ;
    iao:IAO_0000115 "The ability of muscle groups to perform repeated contractions over time. [ACSM 2018]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d430> .

pafh:FlexibilityFitness a owl:Class ;
    rdfs:label "Flexibility" ;
    rdfs:subClassOf pafh:HealthRelatedFitness ;
    iao:IAO_0000115 "The range of motion available at a joint. [ACSM 2018]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d410> .

# Skill-Related Fitness
pafh:SkillRelatedFitness a owl:Class ;
    rdfs:label "Skill-Related Fitness" ;
    rdfs:subClassOf pafh:PhysicalFitness ;
    iao:IAO_0000115 "A dimension of physical fitness related to abilities needed for sports performance, such as agility, balance, coordination, power, reaction time, and speed. [ACSM 2018]"^^xsd:string .

pafh:Agility a owl:Class ;
    rdfs:label "Agility" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Ability to rapidly change the position of the entire body in space with speed and precision. [ACSM 2018]"^^xsd:string .

pafh:Balance a owl:Class ;
    rdfs:label "Balance" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Ability to maintain the body's center of gravity within the base of support. [ACSM 2018]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d415> .

pafh:Proprioception a owl:Class ;
    rdfs:label "Proprioception" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "The sense of the relative position and movement of body parts, enabling coordination and balance. Proprioception is essential for skilled movement and injury prevention. [PMID:20401789, ACSM 2018]"^^xsd:string .

pafh:Coordination a owl:Class ;
    rdfs:label "Coordination" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Ability to use different parts of the body together smoothly and efficiently. [ACSM 2018]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/ICF_d440> .

pafh:Power a owl:Class ;
    rdfs:label "Power" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Ability to exert maximum muscular contraction instantly in an explosive movement. [ACSM 2018]"^^xsd:string .

pafh:Acceleration a owl:Class ;
    rdfs:label "Acceleration" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "The rate at which an individual can increase the velocity of movement. Acceleration is a key component of performance in many sports and activities. [ACSM 2018]"^^xsd:string .

pafh:ReactionTime a owl:Class ;
    rdfs:label "Reaction Time" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Time elapsed between the presentation of a stimulus and the initiation of a response. [ACSM 2018]"^^xsd:string .

pafh:Speed a owl:Class ;
    rdfs:label "Speed" ;
    rdfs:subClassOf pafh:SkillRelatedFitness ;
    iao:IAO_0000115 "Ability to perform a movement within a short period of time. [ACSM 2018]"^^xsd:string .


pafh:MusculoskeletalFitness a owl:Class ;
    rdfs:label "Musculoskeletal Fitness" ;
    rdfs:subClassOf pafh:HealthRelatedFitness ;
    iao:IAO_0000115 "Fitness related to the strength, endurance, and function of muscles and bones. [ACSM 2018]"^^xsd:string .

pafh:PopulationRelatedFitness a owl:Class ;
    rdfs:label "Population-related Fitness" ;
    rdfs:subClassOf pafh:PhysicalFitness ;
    iao:IAO_0000115 "Fitness attributes and measures relevant at the population level, such as prevalence of fitness or health indicators. [CDC 2023]"^^xsd:string .

pafh:Recovery a owl:Class ;
    rdfs:label "Rest and Recovery" ;
    rdfs:subClassOf pafh:PhysicalFitness ;
    iao:IAO_0000115 "Processes and attributes related to the restoration of function and reduction of fatigue following physical activity. [ACSM 2018]"^^xsd:string .

pafh:hasComponent a owl:ObjectProperty ;
    rdfs:label "has component" ;
    rdfs:domain pafh:PhysicalFitness ;
    rdfs:range pafh:PhysicalFitness ;
    rdfs:comment "Relates a physical fitness entity to its component fitness dimensions (e.g., health-related, skill-related, musculoskeletal)." .

# Example usage for clarity:
pafh:PhysicalFitness pafh:hasComponent pafh:HealthRelatedFitness, pafh:SkillRelatedFitness, pafh:MusculoskeletalFitness, pafh:PopulationRelatedFitness, pafh:Recovery .

# --- END PHYSICAL FITNESS BRANCH ---

# --- HEALTH BRANCH ---

pafh:Health a owl:Class ;
    rdfs:label "Health" ;
    rdfs:subClassOf bfo:Disposition ;
    iao:IAO_0000115 "A state of complete physical, mental, and social well-being and not merely the absence of disease or infirmity. [WHO 1948, PMID:15003161]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/OGMS_0000045> ;
    skos:closeMatch <http://purl.bioontology.org/ontology/SNOMEDCT/36456004> .

pafh:PhysiologicalHealth a owl:Class ;
    rdfs:label "Physiological Health" ;
    rdfs:subClassOf pafh:Health ;
    iao:IAO_0000115 "The state and function of the body's biological systems, including cardiovascular, respiratory, musculoskeletal, metabolic, and immune systems. [PMID:15003161]"^^xsd:string ;
    skos:closeMatch <http://purl.obolibrary.org/obo/OGMS_0000097> .

pafh:CardiovascularHealth a owl:Class ;
    rdfs:label "Cardiovascular Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the cardiovascular system of an organism to support circulation and oxygen delivery, realized by the coordinated function of the heart and blood vessels. [MedlinePlus, CDC 2023, BFO]"^^xsd:string .

pafh:HeartHealth a owl:Class ;
    rdfs:label "Heart Health" ;
    rdfs:subClassOf pafh:CardiovascularHealth ;
    iao:IAO_0000115 "A disposition of the heart of an organism to pump blood throughout the body, realized by cardiac muscle contraction and coordinated electrical activity. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:BloodVesselHealth a owl:Class ;
    rdfs:label "Blood Vessel Health" ;
    rdfs:subClassOf pafh:CardiovascularHealth ;
    iao:IAO_0000115 "A disposition of the blood vessels of an organism to transport blood, nutrients, gases, and wastes to and from tissues, realized by the function of arteries, veins, and capillaries. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:RespiratoryHealth a owl:Class ;
    rdfs:label "Respiratory Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the respiratory system of an organism to support gas exchange and respiration, realized by the coordinated function of the lungs and airways. [MedlinePlus, CDC 2023, BFO]"^^xsd:string .

pafh:LungHealth a owl:Class ;
    rdfs:label "Lung Health" ;
    rdfs:subClassOf pafh:RespiratoryHealth ;
