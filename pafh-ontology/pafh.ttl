pafh:TracheaHealth a owl:Class ;
    rdfs:label "Trachea Health" ;
    rdfs:subClassOf pafh:RespiratoryHealth ;
    iao:IAO_0000115 "A disposition of the trachea to conduct air to and from the lungs, realized by the function of its cartilaginous structure and lining. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:LarynxHealth a owl:Class ;
    rdfs:label "Larynx Health" ;
    rdfs:subClassOf pafh:RespiratoryHealth ;
