pafh:UterusHealth a owl:Class ;
    rdfs:label "Uterus Health" ;
    rdfs:subClassOf pafh:ReproductiveSystemHealth ;
    iao:IAO_0000115 "A disposition of the uterus to support fetal development and menstruation, realized by the function of endometrial and myometrial tissues. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:LymphaticSystemHealth a owl:Class ;
    rdfs:label "Lymphatic System Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the lymphatic system of an organism to support immune function and fluid balance, realized by the function of lymph nodes, lymph vessels, spleen, and thymus. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:LymphNodeHealth a owl:Class ;
    rdfs:label "Lymph Node Health" ;
    rdfs:subClassOf pafh:LymphaticSystemHealth ;
