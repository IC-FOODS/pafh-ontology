# Direct observation methods
pafh:DirectObservationMethod a owl:Class ;
    rdfs:label "Direct Observation Method" ;
    rdfs:subClassOf pafh:MeasurementMethod ;
    iao:IAO_0000115 "A measurement method involving direct observation of physical activity, fitness, or related behaviors by trained observers or via recording devices. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:InPersonObservationMethod a owl:Class ;
    rdfs:label "In-Person Observation Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method where trained observers assess physical activity or related behaviors in real time and in person. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:VideoObservationMethod a owl:Class ;
    rdfs:label "Video Observation Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method using video recordings to assess physical activity or related behaviors. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:WearableCameraMethod a owl:Class ;
    rdfs:label "Wearable Camera Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method using wearable cameras to capture and assess physical activity or related behaviors from a first-person perspective. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

pafh:AutomatedVideoAnalysisMethod a owl:Class ;
    rdfs:label "Automated Video Analysis Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method using automated video analysis (e.g., computer vision, AI) to assess physical activity or related behaviors from recorded footage. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

pafh:DroneObservationMethod a owl:Class ;
    rdfs:label "Drone Observation Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method using aerial drones to observe and record physical activity in outdoor or large-scale settings. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

pafh:CrowdSourcedObservationMethod a owl:Class ;
    rdfs:label "Crowd-Sourced Observation Method" ;
    rdfs:subClassOf pafh:DirectObservationMethod ;
    iao:IAO_0000115 "A direct observation method utilizing crowd-sourced data or observations (e.g., public event tracking, citizen science) to assess physical activity. [PMID:33577446; WHO]"^^xsd:string ;
    dcterms:source "PMID:33577446; WHO" .

# Laboratory methods
pafh:LaboratoryMethod a owl:Class ;
    rdfs:label "Laboratory Method" ;
    rdfs:subClassOf pafh:MeasurementMethod ;
    iao:IAO_0000115 "A measurement method conducted in a laboratory setting to assess physical activity, fitness, or health-related variables using specialized equipment or protocols. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:VO2MaxTestMethod a owl:Class ;
    rdfs:label "VO2 Max Test Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method measuring maximal oxygen uptake (VO2 max) as an indicator of cardiorespiratory fitness. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:BloodBiomarkerMethod a owl:Class ;
    rdfs:label "Blood Biomarker Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method involving analysis of blood samples to assess physiological or metabolic markers related to physical activity or fitness. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:DEXAMethod a owl:Class ;
    rdfs:label "DEXA Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using dual-energy X-ray absorptiometry (DEXA) to assess body composition, bone density, or related outcomes. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:LactateThresholdTestMethod a owl:Class ;
    rdfs:label "Lactate Threshold Test Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method measuring blood lactate levels during graded exercise to determine the lactate threshold, an indicator of aerobic endurance. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:SpirometryMethod a owl:Class ;
    rdfs:label "Spirometry Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using spirometry to assess lung function and respiratory parameters during or after physical activity. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:IsokineticDynamometryMethod a owl:Class ;
    rdfs:label "Isokinetic Dynamometry Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using isokinetic dynamometry to assess muscle strength and power at constant speed. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:GenomicAssayMethod a owl:Class ;
    rdfs:label "Genomic Assay Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using genomic assays to assess genetic markers or expression profiles related to physical activity or fitness. [PMID:17956337; WHO]"^^xsd:string ;
    dcterms:source "PMID:17956337; WHO" .

pafh:MetabolicCartMethod a owl:Class ;
    rdfs:label "Metabolic Cart Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using a metabolic cart to measure oxygen consumption, carbon dioxide production, and energy expenditure. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:UltrasoundImagingMethod a owl:Class ;
    rdfs:label "Ultrasound Imaging Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using ultrasound imaging to assess muscle, tendon, or fat thickness, or vascular function. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:MRIMethod a owl:Class ;
    rdfs:label "MRI Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using magnetic resonance imaging (MRI) to assess muscle or fat volume, organ size, or tissue composition. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:CTMethod a owl:Class ;
    rdfs:label "CT Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using computed tomography (CT) to obtain cross-sectional images for muscle or fat distribution. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:NearInfraredSpectroscopyMethod a owl:Class ;
    rdfs:label "Near-Infrared Spectroscopy Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using near-infrared spectroscopy (NIRS) to measure muscle oxygenation during exercise. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:ElectrocardiographyMethod a owl:Class ;
    rdfs:label "Electrocardiography Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using electrocardiography (ECG) to assess cardiac function during physical activity or exercise testing. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:ElectroencephalographyMethod a owl:Class ;
    rdfs:label "Electroencephalography Method" ;
    rdfs:subClassOf pafh:LaboratoryMethod ;
    iao:IAO_0000115 "A laboratory method using electroencephalography (EEG) to assess brain activity during physical activity or cognitive tasks. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

# Protocol-based methods
pafh:ProtocolBasedMethod a owl:Class ;
    rdfs:label "Protocol-Based Method" ;
    rdfs:subClassOf pafh:MeasurementMethod ;
    iao:IAO_0000115 "A measurement method involving standardized protocols or field tests to assess physical activity, fitness, or functional capacity. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:SixMinuteWalkTestMethod a owl:Class ;
    rdfs:label "Six-Minute Walk Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using the six-minute walk test to assess functional exercise capacity. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:ShuttleRunTestMethod a owl:Class ;
    rdfs:label "Shuttle Run Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using shuttle run tests (e.g., beep test) to assess aerobic fitness or endurance. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:SitToStandTestMethod a owl:Class ;
    rdfs:label "Sit-to-Stand Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using sit-to-stand tests to assess lower-body strength or functional mobility. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:TimedUpAndGoTestMethod a owl:Class ;
    rdfs:label "Timed Up and Go Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using the Timed Up and Go (TUG) test to assess mobility, balance, and fall risk. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:GripStrengthTestMethod a owl:Class ;
    rdfs:label "Grip Strength Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using grip strength testing to assess upper-body muscular strength. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:StepTestMethod a owl:Class ;
    rdfs:label "Step Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using step tests (e.g., Harvard Step Test) to assess cardiovascular fitness or endurance. [PMID:20401789; WHO]"^^xsd:string ;
    dcterms:source "PMID:20401789; WHO" .

pafh:FieldBasedAgilityTestMethod a owl:Class ;
    rdfs:label "Field-Based Agility Test Method" ;
    rdfs:subClassOf pafh:ProtocolBasedMethod ;
    iao:IAO_0000115 "A protocol-based method using field-based agility tests (e.g., T-test, Illinois agility test) to assess agility and movement skill. [PMID:20401789; WHO]"^^xsd:string ;
