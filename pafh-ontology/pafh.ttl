pafh:SpleenHealth a owl:Class ;
    rdfs:label "Spleen Health" ;
    rdfs:subClassOf pafh:LymphaticSystemHealth ;
    iao:IAO_0000115 "A disposition of the spleen to filter blood and support immune function, realized by the function of white and red pulp. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:ThymusHealth a owl:Class ;
    rdfs:label "Thymus Health" ;
    rdfs:subClassOf pafh:LymphaticSystemHealth ;
    iao:IAO_0000115 "A disposition of the thymus to mature T-cells and support immune development, realized by the function of thymic epithelial cells. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:TonsilHealth a owl:Class ;
    rdfs:label "Tonsil Health" ;
    rdfs:subClassOf pafh:LymphaticSystemHealth ;
    iao:IAO_0000115 "A disposition of the tonsils to trap pathogens and support immune responses, realized by the function of lymphoid follicles. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:SensorySystemHealth a owl:Class ;
