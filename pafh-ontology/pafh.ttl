# --- SALUTOLOGY BRANCH ---

pafh:Salutology a owl:Class ;
    rdfs:label "Salutology" ;
    iao:IAO_0000115 "The study and science of health, focusing on the origins, processes, and maintenance of health and well-being, as opposed to the study of disease (pathology). [Antonovsky 1979; WHO]"^^xsd:string ;
    rdfs:comment "Salutology (or salutogenesis) is the counterpart to pathology in health science." .

pafh:SalutogenicProcess a owl:Class ;
    rdfs:label "Salutogenic Process" ;
    iao:IAO_0000115 "A process that supports, promotes, or maintains health and well-being in an organism, realized by biological, psychological, or social mechanisms. [Antonovsky 1979; WHO; BFO]"^^xsd:string ;
    rdfs:comment "Counterpart to pathological process; includes adaptation, resilience, and recovery mechanisms." .

pafh:SalutogenicBehaviour a owl:Class ;
    rdfs:label "Salutogenic Behaviour" ;
    rdfs:subClassOf pafh:SalutogenicProcess ;
    iao:IAO_0000115 "A behaviour or pattern of action that promotes or maintains health and well-being, such as regular physical activity, balanced nutrition, or effective stress management. [WHO, CDC]"^^xsd:string .

pafh:MindfulnessPractice a owl:Class ;
    rdfs:label "Mindfulness Practice" ;
    rdfs:subClassOf pafh:SalutogenicBehaviour ;
    iao:IAO_0000115 "A salutogenic behaviour involving regular engagement in mindfulness or meditation practices that support mental and physical well-being. [WHO, CDC]"^^xsd:string .

pafh:VolunteeringBehaviour a owl:Class ;
    rdfs:label "Volunteering (Behaviour)" ;
    rdfs:subClassOf pafh:SalutogenicBehaviour ;
    iao:IAO_0000115 "A salutogenic behaviour involving voluntary, unpaid activities that benefit others or the community, fostering social connection and purpose. [WHO, CDC]"^^xsd:string .

pafh:SleepHygiene a owl:Class ;
    rdfs:label "Sleep Hygiene" ;
    rdfs:subClassOf pafh:SalutogenicBehaviour ;
    iao:IAO_0000115 "A salutogenic behaviour involving practices that promote regular, restful, and adequate sleep to support health. [CDC, NIH]"^^xsd:string .

pafh:SalutogenicFactor a owl:Class ;
    rdfs:label "Salutogenic Factor" ;
    rdfs:subClassOf pafh:SalutogenicProcess ;
    iao:IAO_0000115 "A variable, condition, or resource that contributes to the promotion or maintenance of health and well-being. [Antonovsky 1979; WHO]"^^xsd:string ;
    rdfs:comment "Examples: social support, physical activity, nutrition, sense of coherence." .

pafh:SocialSupport a owl:Class ;
    rdfs:label "Social Support" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor consisting of emotional, informational, or practical assistance received from others, contributing to health and well-being. [WHO, CDC]"^^xsd:string .

pafh:PhysicalActivityFactor a owl:Class ;
    rdfs:label "Physical Activity (Salutogenic Factor)" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving regular bodily movement that enhances or maintains physical and mental health. [WHO, CDC]"^^xsd:string .

pafh:Nutrition a owl:Class ;
    rdfs:label "Nutrition" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving adequate and balanced intake of nutrients that supports growth, function, and health. [WHO, CDC]"^^xsd:string .

pafh:HealthyDiet a owl:Class ;
    rdfs:label "Healthy Diet" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving dietary patterns rich in fruits, vegetables, whole grains, and lean proteins that support health and prevent disease. [WHO, CDC]"^^xsd:string .

pafh:SleepQuality a owl:Class ;
    rdfs:label "Sleep Quality" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving sufficient, restorative, and regular sleep that supports physical and mental health. [CDC, NIH]"^^xsd:string .

pafh:PurposeInLife a owl:Class ;
    rdfs:label "Purpose in Life" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving a sense of direction and meaning that motivates health-promoting behaviors and resilience. [WHO, CDC]"^^xsd:string .

pafh:Optimism a owl:Class ;
    rdfs:label "Optimism" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving a positive outlook and expectation of favorable outcomes, which supports coping and health. [WHO, CDC]"^^xsd:string .

pafh:PositiveRelationships a owl:Class ;
    rdfs:label "Positive Relationships" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving supportive, trusting, and meaningful social connections that enhance well-being. [WHO, CDC]"^^xsd:string .

pafh:CommunityEngagement a owl:Class ;
    rdfs:label "Community Engagement" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving participation in community activities and civic life that fosters belonging and collective efficacy. [WHO, CDC]"^^xsd:string .

pafh:Volunteering a owl:Class ;
    rdfs:label "Volunteering" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving unpaid, intentional service to others or the community that promotes social connection and purpose. [WHO, CDC]"^^xsd:string .

pafh:SelfEfficacy a owl:Class ;
    rdfs:label "Self-Efficacy" ;
    rdfs:subClassOf pafh:SalutogenicFactor ;
    iao:IAO_0000115 "A salutogenic factor involving the belief in one's ability to succeed in specific situations or accomplish tasks, supporting health behaviors. [WHO, CDC]"^^xsd:string .

pafh:SalutogenicResource a owl:Class ;
    rdfs:label "Salutogenic Resource" ;
    rdfs:subClassOf pafh:SalutogenicProcess ;
    iao:IAO_0000115 "A tangible or intangible resource that enables or enhances salutogenic processes and outcomes. [Antonovsky 1979; WHO]"^^xsd:string ;
    rdfs:comment "Examples: education, access to healthcare, coping skills." .

pafh:AccessToHealthcare a owl:Class ;
    rdfs:label "Access to Healthcare" ;
    rdfs:subClassOf pafh:SalutogenicResource ;
    iao:IAO_0000115 "A salutogenic resource involving the availability and use of preventive and curative health services. [WHO, CDC]"^^xsd:string .

pafh:CopingSkills a owl:Class ;
    rdfs:label "Coping Skills" ;
    rdfs:subClassOf pafh:SalutogenicResource ;
    iao:IAO_0000115 "A salutogenic resource involving psychological and behavioral strategies to manage stress and adversity. [WHO, CDC]"^^xsd:string .

pafh:PreventiveCare a owl:Class ;
    rdfs:label "Preventive Care" ;
    rdfs:subClassOf pafh:SalutogenicResource ;
    iao:IAO_0000115 "A salutogenic resource involving health services and interventions that prevent disease or detect it early, supporting long-term health. [WHO, CDC]"^^xsd:string .

pafh:AdequateRest a owl:Class ;
    rdfs:label "Adequate Rest" ;
    rdfs:subClassOf pafh:SalutogenicResource ;
    iao:IAO_0000115 "A salutogenic resource involving sufficient downtime and recovery periods that allow for physical and psychological restoration. [WHO, CDC]"^^xsd:string .

pafh:SalutogenicMechanism a owl:Class ;
    rdfs:label "Salutogenic Mechanism" ;
    rdfs:subClassOf pafh:SalutogenicProcess ;
    iao:IAO_0000115 "A biological, psychological, or social mechanism by which salutogenic factors exert effects on health. [Antonovsky 1979; WHO]"^^xsd:string ;
    rdfs:comment "Examples: stress adaptation, immune enhancement, neuroplasticity." .

pafh:StressManagement a owl:Class ;
    rdfs:label "Stress Management" ;
    rdfs:subClassOf pafh:SalutogenicMechanism ;
    iao:IAO_0000115 "A salutogenic mechanism involving strategies and physiological processes that reduce the negative effects of stress and support health. [WHO, CDC]"^^xsd:string .

pafh:EmotionalRegulation a owl:Class ;
    rdfs:label "Emotional Regulation" ;
    rdfs:subClassOf pafh:SalutogenicMechanism ;
    iao:IAO_0000115 "A salutogenic mechanism involving the ability to monitor, evaluate, and modify emotional reactions to achieve goals and maintain well-being. [WHO, CDC]"^^xsd:string .

pafh:Mindfulness a owl:Class ;
    rdfs:label "Mindfulness" ;
    rdfs:subClassOf pafh:SalutogenicMechanism ;
    iao:IAO_0000115 "A salutogenic mechanism involving purposeful, non-judgmental attention to present experiences, supporting mental and physical health. [WHO, CDC]"^^xsd:string .

pafh:SalutogenicOutcome a owl:Class ;
    rdfs:label "Salutogenic Outcome" ;
    rdfs:subClassOf pafh:SalutogenicProcess ;
    iao:IAO_0000115 "A positive health state or improvement resulting from salutogenic processes or factors. [Antonovsky 1979; WHO]"^^xsd:string ;
    rdfs:comment "Examples: resilience, well-being, functional capacity, recovery." .

