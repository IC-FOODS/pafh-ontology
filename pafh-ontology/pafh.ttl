pafh:UrinarySystemHealth a owl:Class ;
    rdfs:label "Urinary System Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the urinary system of an organism to filter blood and eliminate waste, realized by the function of the kidneys, bladder, and associated structures. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:KidneyHealth a owl:Class ;
