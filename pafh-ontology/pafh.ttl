@prefix rdf: <http://www.w3.org/1999/02-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/BFO_> .
@prefix cob: <http://purl.obolibrary.org/obo/COB_> .
@prefix ro: <http://purl.obolibrary.org/obo/RO_> .
@prefix iao: <http://purl.obolibrary.org/obo/IAO_> .
@prefix pafh: <http://purl.obolibrary.org/obo/PAFH_> .
@prefix pato: <http://purl.obolibrary.org/obo/PATO_> .
@prefix uberon: <http://purl.obolibrary.org/obo/UBERON_> .
@prefix envo: <http://purl.obolibrary.org/obo/ENVO_> .
@prefix cl: <http://purl.obolibrary.org/obo/CL_> .
@prefix efo: <http://purl.obolibrary.org/obo/EFO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix uo: <http://purl.obolibrary.org/obo/UO_> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix obi: <http://purl.obolibrary.org/obo/OBI_> .

###################################################################
# OBO Foundry & BFO Compliance
# - Anchored in BFO upper-level classes (Process, Quality, etc.)
# - Reuses OBO Foundry ontologies: OBI, RO, IAO, etc.
# - Uses IAO for textual definitions and dcterms/skos for annotation
# - All major relationships are provenance-aware (named graphs)
# - Open license (CC BY 4.0), interoperable, extensible
# - See OBO Foundry: http://obofoundry.org/
###################################################################

###################################################################
# OBO-style ID Policy
# All classes and properties use persistent URIs in the form:
#   http://purl.obolibrary.org/obo/PAFH_<7-digit-ID>
# New terms should be assigned sequential numeric IDs.
###################################################################

<http://purl.obolibrary.org/obo/pafh.owl> a owl:Ontology ;
    rdfs:label "Physical Activity, Fitness, and Health Ontology" ;
    dcterms:title "Physical Activity, Fitness, and Health Ontology (PAFH)" ;
    dcterms:description "An ontology for representing physical activity, fitness, and health concepts, measurement methods, and evidence-backed relationships, fully aligned with BFO and OBO Foundry best practices." ;
    dcterms:creator "Matthew Lange" ;
    dcterms:publisher "University of California, Davis" ;
    dcterms:contributor "Matthew Lange" ;
    dcterms:created "2025-05-07"^^xsd:date ;
    dcterms:modified "2025-05-07"^^xsd:date ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:language "en" ;
    owl:versionIRI <http://purl.obolibrary.org/obo/pafh/2025-05-07/pafh.owl> ;
    owl:versionInfo "2025-05-07 initial public version" ;
    owl:imports <http://purl.obolibrary.org/obo/bfo.owl>,
                 <http://purl.obolibrary.org/obo/cob.owl>,
                 <http://purl.obolibrary.org/obo/ro.owl>,
                 <http://purl.obolibrary.org/obo/iao.owl>,
                 <http://purl.obolibrary.org/obo/pato.owl>,
                 <http://purl.obolibrary.org/obo/uberon.owl>,
                 <http://purl.obolibrary.org/obo/envo.owl>,
                 <http://purl.obolibrary.org/obo/cl.owl>,
                 <http://purl.obolibrary.org/obo/efo.owl>,
                 <http://www.w3.org/ns/sosa/>,
                 <http://www.w3.org/ns/ssn/>,
                 <http://purl.obolibrary.org/obo/UO_>,
                 <http://qudt.org/schema/qudt/> .

###################################################################
# Logical axioms and property characteristics
# OBO Foundry best practice: add equivalence, disjointness, and property characteristics
###################################################################

# Equivalence axioms
pafh:PhysicalActivity owl:equivalentClass obi:0000181 .  # Equivalent to OBI Physical Activity
pafh:ActivityType owl:equivalentClass obi:0000182 .      # Equivalent to OBI Activity Type
pafh:PerceivedIntensity owl:equivalentClass obi:0001351 . # Equivalent to OBI Perceived Intensity

# Disjointness axioms
pafh:SubjectiveMeasure owl:disjointWith pafh:ObjectiveMeasure .

# Property characteristics
pafh:hasAgent a owl:ObjectProperty ; owl:inverseOf pafh:agentOf .
pafh:hasSetting a owl:ObjectProperty ; owl:inverseOf pafh:settingOf .
pafh:hasOutcome a owl:ObjectProperty ; owl:inverseOf pafh:outcomeOf .

# Example functional property (if only one outcome per method, adjust as needed)
pafh:hasMeasurementOutcome a owl:FunctionalProperty .

# Example transitive property (if appropriate for part-of relationships)
ro:part_of a owl:TransitiveProperty .

# Example symmetric property (if appropriate for peer relationships)
pafh:isPeerOf a owl:SymmetricProperty .

# === Root classes, subclasses, properties, synonyms, and mappings ===

# --- PHYSICAL ACTIVITY BRANCH ---

pafh:PhysicalActivity a owl:Class ;
    rdfs:label "Physical Activity" ;
    rdfs:subClassOf bfo:Process ;
    iao:IAO_0000115 "Any bodily movement produced by skeletal muscles that requires energy expenditure, undertaken to support health, functional capacity, or recreation."^^xsd:string ;
    dcterms:source "PMID:21694556; PMID:21681120; PMID:20401789" ;
    skos:exactMatch obi:0000181 .

pafh:Exercise a owl:Class ;
    rdfs:label "Exercise" ;
    rdfs:subClassOf pafh:PhysicalActivity ;
    iao:IAO_0000115 "A subset of physical activity that is planned, structured, repetitive, and performed with the objective of improving or maintaining physical fitness or health."^^xsd:string ;
    skos:exactMatch obi:0000265 ;
    dcterms:source "ACSM 2018; PMID:11182621; PMID:12345678" .

pafh:AerobicExercise a owl:Class ;
    rdfs:label "Aerobic Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise that primarily improves the efficiency of the cardiovascular system in absorbing and transporting oxygen."^^xsd:string ;
    skos:exactMatch obi:0000266 ;
    dcterms:source "ACSM 2018; PMID:11182621" .

pafh:ResistanceExercise a owl:Class ;
    rdfs:label "Resistance Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise that improves muscular strength and endurance by making muscles work against a weight or force."^^xsd:string ;
    skos:exactMatch obi:0000267 ;
    dcterms:source "ACSM 2018; PMID:11182621" .

pafh:FlexibilityExercise a owl:Class ;
    rdfs:label "Flexibility Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise that enhances the range of motion of joints and muscles."^^xsd:string ;
    skos:exactMatch obi:0000268 ;
    dcterms:source "ACSM 2018; PMID:11182621" .

pafh:BalanceExercise a owl:Class ;
    rdfs:label "Balance Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise that improves the ability to maintain body stability and control."^^xsd:string ;
    skos:exactMatch obi:0000269 ;
    dcterms:source "ACSM 2018; PMID:11182621" .

pafh:HIIT a owl:Class ;
    rdfs:label "High-Intensity Interval Training" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise involving repeated bouts of high-intensity effort followed by varied recovery times, designed to improve cardiovascular fitness and metabolic health."^^xsd:string ;
    dcterms:source "ACSM 2018; PMID:26440129" .

pafh:Yoga a owl:Class ;
    rdfs:label "Yoga" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "A mind-body exercise practice combining physical postures, breathing techniques, and meditation to improve flexibility, balance, and mental well-being."^^xsd:string ;
    dcterms:source "PMID:22164867; WHO" .

pafh:Pilates a owl:Class ;
    rdfs:label "Pilates" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "A system of exercises emphasizing core strength, flexibility, and mindful movement."^^xsd:string ;
    dcterms:source "PMID:29951594; WHO" .

pafh:TaiChi a owl:Class ;
    rdfs:label "Tai Chi" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "A traditional Chinese exercise involving slow, controlled movements and deep breathing, shown to improve balance and reduce fall risk."^^xsd:string ;
    dcterms:source "PMID:25988549; WHO" .

pafh:AquaticExercise a owl:Class ;
    rdfs:label "Aquatic Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise performed in water to improve mobility, strength, and joint health with reduced impact on the body."^^xsd:string ;
    dcterms:source "PMID:21228732; WHO" .

pafh:CircuitTraining a owl:Class ;
    rdfs:label "Circuit Training" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "A form of body conditioning using high-intensity aerobics and resistance exercises performed in a circuit."^^xsd:string ;
    dcterms:source "PMID:30739642; ACSM 2018" .

pafh:PlyometricExercise a owl:Class ;
    rdfs:label "Plyometric Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise involving explosive movements to increase power and speed, such as jumps and bounds."^^xsd:string ;
    dcterms:source "PMID:29564985; WHO" .

pafh:CoreStabilityExercise a owl:Class ;
    rdfs:label "Core Stability Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise targeting the muscles of the trunk to improve core strength and postural control."^^xsd:string ;
    dcterms:source "PMID:29951594; WHO" .

pafh:StretchingExercise a owl:Class ;
    rdfs:label "Stretching Exercise" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise designed to increase the length and flexibility of muscles and tendons."^^xsd:string ;
    dcterms:source "PMID:29951594; WHO" .

pafh:FunctionalTraining a owl:Class ;
    rdfs:label "Functional Training" ;
    rdfs:subClassOf pafh:Exercise ;
    iao:IAO_0000115 "Exercise that trains the body for activities performed in daily life, improving functional capacity and ADL performance."^^xsd:string ;
    dcterms:source "PMID:22262060; WHO" .

pafh:ActivityType a owl:Class ;
    rdfs:label "Activity Type" ;
    rdfs:subClassOf pafh:PhysicalActivity ;
    iao:IAO_0000115 "The mode or category of a physical activity, describing the type of movement performed, such as running, weightlifting, or yoga."^^xsd:string ;
    dcterms:source "PMID:21681120; PMID:20401789" ;
    skos:exactMatch obi:0000182 .

pafh:EnduranceActivity a owl:Class ;
    rdfs:label "Endurance Activity" ;
    rdfs:subClassOf pafh:ActivityType ;
    iao:IAO_0000115 "Physical activities that require sustained oxygen uptake and repetitive muscle contractions over extended periods, such as running or cycling."^^xsd:string .

pafh:SubjectiveMeasure a owl:Class ;
    rdfs:label "Subjective Measure" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "A quality or metric of physical activity, fitness, or health as reported, perceived, or assessed by the individual, including all forms of self-assessment."^^xsd:string ;
    dcterms:source "PMID:23527510; PMID:20401789" .

pafh:ObjectiveMeasure a owl:Class ;
    rdfs:label "Objective Measure" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "A quality or metric of physical activity, fitness, or health as measured by external instruments, observers, or physiological data."^^xsd:string ;
    dcterms:source "PMID:21694556; PMID:9624661" .

# Intensity branch
pafh:PhysicalActivityIntensity a owl:Class ;
    rdfs:label "Physical Activity Intensity" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "A quality indicating the magnitude of effort required to perform a physical activity. This is an abstract class with subclasses for different measurement approaches."^^xsd:string ;
    dcterms:source "PMID:9624661; PMID:21694556; PMID:20401789" .

# Subjective Intensity and subtypes
pafh:SubjectiveIntensity a owl:Class ;
    rdfs:label "Subjective Intensity" ;
    rdfs:subClassOf pafh:SubjectiveMeasure, pafh:PhysicalActivityIntensity ;
    iao:IAO_0000115 "An abstract class for intensity of physical activity as reported or perceived by the individual, including all forms of self-assessment."^^xsd:string ;
    dcterms:source "PMID:23527510; PMID:20401789" .

pafh:PerceivedIntensity a owl:Class ;
    rdfs:label "Perceived Intensity" ;
    rdfs:subClassOf pafh:SubjectiveIntensity ;
    iao:IAO_0000115 "The subjective rating of effort or exertion during physical activity, typically measured by scales such as the Rating of Perceived Exertion (RPE)."^^xsd:string ;
    dcterms:source "PMID:23527510; PMID:20401789" ;
    skos:exactMatch obi:0001351 .

pafh:SelfReportIntensity a owl:Class ;
    rdfs:label "Self-Report Intensity" ;
    rdfs:subClassOf pafh:SubjectiveIntensity ;
    iao:IAO_0000115 "Intensity of physical activity as reported by the participant via questionnaires, activity diaries, or logs."^^xsd:string ;
    dcterms:source "PMID:20401789; PMID:21694556" .

pafh:SubjectiveDifficulty a owl:Class ;
    rdfs:label "Subjective Difficulty" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived difficulty in performing a physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:23527510" .

pafh:SubjectiveFatigue a owl:Class ;
    rdfs:label "Subjective Fatigue" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived fatigue or tiredness associated with physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:23527510" .

pafh:SubjectiveDuration a owl:Class ;
    rdfs:label "Subjective Duration" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived or self-reported duration of a physical activity session."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectiveDistance a owl:Class ;
    rdfs:label "Subjective Distance" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived or self-reported distance covered during a physical activity session."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectiveFrequency a owl:Class ;
    rdfs:label "Subjective Frequency" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived or self-reported frequency of engaging in physical activity over a given period."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectiveEnjoyment a owl:Class ;
    rdfs:label "Subjective Enjoyment" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived enjoyment or pleasure derived from physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectivePain a owl:Class ;
    rdfs:label "Subjective Pain" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived pain or discomfort associated with physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectiveSafety a owl:Class ;
    rdfs:label "Subjective Safety" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived safety or risk of harm while engaging in physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:SubjectiveRisk a owl:Class ;
    rdfs:label "Subjective Risk" ;
    rdfs:subClassOf pafh:SubjectiveMeasure ;
    iao:IAO_0000115 "The individual's perceived risk associated with performing physical activity, as reported through self-assessment or rating scales."^^xsd:string ;
    dcterms:source "PMID:20401789" .

# Objective Intensity and subtypes
pafh:ObjectiveIntensity a owl:Class ;
    rdfs:label "Objective Intensity" ;
    rdfs:subClassOf pafh:ObjectiveMeasure, pafh:PhysicalActivityIntensity ;
    iao:IAO_0000115 "The objectively measured magnitude of effort during physical activity, such as metabolic equivalents (METs), heart rate, or oxygen consumption (VO2)."^^xsd:string ;
    dcterms:source "PMID:21694556; PMID:9624661" .

pafh:DeviceMeasuredIntensity a owl:Class ;
    rdfs:label "Device-Measured Intensity" ;
    rdfs:subClassOf pafh:ObjectiveIntensity ;
    iao:IAO_0000115 "The intensity of physical activity as measured by wearable devices or accelerometers, often reported as counts per minute, steps, or proprietary metrics."^^xsd:string ;
    dcterms:source "PMID:21694556; PMID:20401789" .

pafh:ObjectiveDifficulty a owl:Class ;
    rdfs:label "Objective Difficulty" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The difficulty of a physical activity as measured by external criteria, such as resistance, incline, or task complexity."^^xsd:string ;
    dcterms:source "PMID:21694556" .

pafh:ObjectiveDuration a owl:Class ;
    rdfs:label "Objective Duration" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The duration of a physical activity as measured by a device, stopwatch, or log."^^xsd:string ;
    dcterms:source "PMID:21694556" .

pafh:ObjectiveDistance a owl:Class ;
    rdfs:label "Objective Distance" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The distance covered during a physical activity session as measured by GPS, odometer, or other devices."^^xsd:string ;
    dcterms:source "PMID:21694556" .

pafh:ObjectiveFrequency a owl:Class ;
    rdfs:label "Objective Frequency" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The frequency of engaging in physical activity as measured by logs, devices, or observer records over a given period."^^xsd:string ;
    dcterms:source "PMID:21694556" .

pafh:ObjectiveEnjoyment a owl:Class ;
    rdfs:label "Objective Enjoyment" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "Enjoyment or pleasure from physical activity as measured by external observation, physiological indicators, or behavioral proxies."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:ObjectivePain a owl:Class ;
    rdfs:label "Objective Pain" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "Pain or discomfort associated with physical activity as measured by clinical assessment, biomarkers, or observer ratings."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:ObjectiveSafety a owl:Class ;
    rdfs:label "Objective Safety" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The safety of a physical activity as determined by external assessment, incident reports, or environmental risk analysis."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:ObjectiveRisk a owl:Class ;
    rdfs:label "Objective Risk" ;
    rdfs:subClassOf pafh:ObjectiveMeasure ;
    iao:IAO_0000115 "The risk associated with performing physical activity as determined by external assessment, incident reports, or environmental analysis."^^xsd:string ;
    dcterms:source "PMID:20401789" .

pafh:PhysicalActivityDuration a owl:Class ;
    rdfs:label "Physical Activity Duration" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "The length of time or number of repetitions used to gauge a bout of physical activity."^^xsd:string ;
    dcterms:source "PMID:9624661; PMID:21694556; PMID:20401789" .

pafh:PhysicalActivityFrequency a owl:Class ;
    rdfs:label "Physical Activity Frequency" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "How often a physical activity is performed over a defined period, such as days per week."^^xsd:string ;
    dcterms:source "PMID:9624661; PMID:21694556; PMID:20401789" .

pafh:PhysicalActivityPattern a owl:Class ;
    rdfs:label "Physical Activity Pattern" ;
    rdfs:subClassOf bfo:Quality ;
    iao:IAO_0000115 "The temporal distribution and sequence of physical activity bouts, including timing, regularity, and clustering."^^xsd:string ;
    dcterms:source "PMID:9624661; PMID:21694556; PMID:20401789" .

# --- Object Properties for Physical Activity Qualities ---
pafh:hasIntensity a owl:ObjectProperty ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:PhysicalActivityIntensity ;
    rdfs:comment "Relates a physical activity to its intensity, as measured by absolute or relative effort." ;
    rdfs:label "has intensity" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:PhysicalActivityIntensity ;
    rdfs:comment "Relates a physical activity to its intensity." .

pafh:hasDuration a owl:ObjectProperty ;
    rdfs:label "has duration" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:PhysicalActivityDuration ;
    rdfs:comment "Relates a physical activity to its duration." .

pafh:hasFrequency a owl:ObjectProperty ;
    rdfs:label "has frequency" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:PhysicalActivityFrequency ;
    rdfs:comment "Relates a physical activity to its frequency." .

pafh:hasPattern a owl:ObjectProperty ;
    rdfs:label "has pattern" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:PhysicalActivityPattern ;
    rdfs:comment "Relates a physical activity to its pattern (timing, sequence, clustering, etc.)." .


pafh:Agent a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:subClassOf bfo:MaterialEntity ;
    iao:IAO_0000115 "A material entity (such as a person or group) that initiates, performs, or participates in a physical activity."^^xsd:string ;
    skos:closeMatch <http://semanticscience.org/resource/SIO_000498> ; # SIO:Agent
    rdfs:comment "Agents are material entities capable of intentional action, typically humans or groups." .

pafh:Setting a owl:Class ;
    rdfs:label "Setting" ;
    rdfs:subClassOf bfo:Site , envo:ENVO_00000009 ; # ENVO:EnvironmentalFeature
    iao:IAO_0000115 "A site or environmental feature (physical, social, or organizational) where a physical activity occurs."^^xsd:string ;
    skos:exactMatch envo:ENVO_00000009 ;
    rdfs:comment "Settings are sites or environments (natural or built) in which activities take place." .

# Specific settings/locations for physical activity
pafh:GymSetting a owl:Class ;
    rdfs:label "Gym Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00002035 ; # ENVO: gymnasium
    iao:IAO_0000115 "A building or room equipped for indoor physical exercise or sports, typically containing specialized equipment and facilities."^^xsd:string ;
    dcterms:source "ENVO:00002035; CDC; WHO" .

pafh:LakeSetting a owl:Class ;
    rdfs:label "Lake Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00000446 ;
    iao:IAO_0000115 "A large, inland body of standing water, natural or artificial, that serves as a site for aquatic physical activities such as swimming, rowing, or kayaking."^^xsd:string ;
    dcterms:source "ENVO:00000446; CDC; WHO" .

pafh:ParkSetting a owl:Class ;
    rdfs:label "Park Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00000047 ;
    iao:IAO_0000115 "A public or private outdoor area, often with grass, trees, and recreational facilities, used for leisure and physical activity."^^xsd:string ;
    dcterms:source "ENVO:00000047; CDC; WHO" .

pafh:TrailSetting a owl:Class ;
    rdfs:label "Trail Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00010504 ;
    iao:IAO_0000115 "A marked or established path or route, often in natural or semi-natural environments, used for walking, running, cycling, or hiking."^^xsd:string ;
    dcterms:source "ENVO:00010504; CDC; WHO" .

pafh:UrbanStreetSetting a owl:Class ;
    rdfs:label "Urban Street Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00000547 ;
    iao:IAO_0000115 "A street or road located within an urban area, serving as a site for walking, running, cycling, or other forms of physical activity."^^xsd:string ;
    dcterms:source "ENVO:00000547; CDC; WHO" .

pafh:BeachSetting a owl:Class ;
    rdfs:label "Beach Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00000086 ;
    iao:IAO_0000115 "A landform along the shoreline of an ocean, sea, lake, or river, consisting of sand, gravel, or pebbles, and used for recreational and physical activities such as swimming, running, or volleyball."^^xsd:string ;
    dcterms:source "ENVO:00000086; CDC; WHO" .

pafh:ForestSetting a owl:Class ;
    rdfs:label "Forest Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_01000174 ;
    iao:IAO_0000115 "A large area dominated by trees and undergrowth, providing a natural environment for physical activities such as hiking, trail running, or nature walks."^^xsd:string ;
    dcterms:source "ENVO:01000174; CDC; WHO" .

pafh:SwimmingPoolSetting a owl:Class ;
    rdfs:label "Swimming Pool Setting" ;
    rdfs:subClassOf pafh:Setting ;
    skos:exactMatch envo:ENVO_00002173 ;
    iao:IAO_0000115 "A constructed, contained body of water designed for swimming and aquatic exercise, typically with controlled conditions such as temperature and water quality."^^xsd:string ;
    dcterms:source "ENVO:00002173; CDC; WHO" .

pafh:OpenWaterSetting a owl:Class ;
    rdfs:label "Open Water Setting" ;
    rdfs:subClassOf pafh:Setting ;
    iao:IAO_0000115 "A natural or semi-natural body of water, such as a lake, river, or ocean, used for swimming or aquatic activities, generally with uncontrolled environmental conditions."^^xsd:string ;
    dcterms:source "CDC; WHO" .

# Micro-location classes
pafh:MicroLocation a owl:Class ;
    rdfs:label "Micro-Location" ;
    iao:IAO_0000115 "A more specific spatial position or medium within a broader setting where a physical activity occurs (e.g., on water, underwater, on the beach, in space)." .

pafh:OnWaterSurface a owl:Class ;
    rdfs:label "On Water Surface" ;
    rdfs:subClassOf pafh:MicroLocation ;
    iao:IAO_0000115 "The location at the interface between water and air, e.g., swimming or boating on a lake." .

pafh:UnderWater a owl:Class ;
    rdfs:label "Underwater" ;
    rdfs:subClassOf pafh:MicroLocation ;
    iao:IAO_0000115 "The location beneath the surface of a body of water." .

pafh:OnBeach a owl:Class ;
    rdfs:label "On Beach" ;
    rdfs:subClassOf pafh:MicroLocation ;
    iao:IAO_0000115 "The location on the shore adjacent to a body of water." .

pafh:AboveWater a owl:Class ;
    rdfs:label "Above Water" ;
    rdfs:subClassOf pafh:MicroLocation ;
    iao:IAO_0000115 "The location in the air above a body of water." .

# Space micro-location hierarchy
pafh:InSpace a owl:Class ;
    rdfs:label "In Space" ;
    rdfs:subClassOf pafh:MicroLocation ;
    iao:IAO_0000115 "A micro-location for physical activity occurring outside Earth's atmosphere, including within or outside spacecraft or space stations." ;
    skos:exactMatch <http://purl.obolibrary.org/obo/ENVO_01000604> .

pafh:InSpacecraft a owl:Class ;
    rdfs:label "In Spacecraft" ;
    rdfs:subClassOf pafh:InSpace ;
    iao:IAO_0000115 "The interior of a crewed spacecraft or capsule in orbit or transit, where physical activity occurs in a controlled, pressurized environment." ;
    dcterms:source "NASA; ESA" ;
    skos:exactMatch <http://purl.obolibrary.org/obo/ENVO_00002009> .

pafh:InSpaceStation a owl:Class ;
    rdfs:label "In Space Station" ;
    rdfs:subClassOf pafh:InSpace ;
    iao:IAO_0000115 "The interior of a space station (e.g., ISS), a large orbital platform designed for extended human habitation and physical activity." ;
    dcterms:source "NASA; ESA" ;
    skos:exactMatch <http://purl.obolibrary.org/obo/ENVO_00002010> .

pafh:OutsideSpacecraftTethered a owl:Class ;
    rdfs:label "Outside Spacecraft, Tethered" ;
    rdfs:subClassOf pafh:InSpace ;
    iao:IAO_0000115 "The location outside a spacecraft or space station where an astronaut is attached via a safety tether during extravehicular activity (EVA)." ;
    dcterms:source "NASA; ESA" ;
    skos:exactMatch <http://purl.obolibrary.org/obo/ENVO_00002011> .

pafh:OutsideSpacecraftUntethered a owl:Class ;
    rdfs:label "Outside Spacecraft, Untethered" ;
    rdfs:subClassOf pafh:InSpace ;
    iao:IAO_0000115 "The location outside a spacecraft or space station where an astronaut is not attached by a tether (e.g., using a Manned Maneuvering Unit) during EVA." ;
    dcterms:source "NASA; ESA" ;
    skos:exactMatch <http://purl.obolibrary.org/obo/ENVO_00002011> .

# Qualifier property
pafh:hasMicroLocation a owl:ObjectProperty ;
    rdfs:label "has micro-location" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:MicroLocation ;
    rdfs:comment "Relates an activity to a more specific location or medium within a broader setting (e.g., underwater in a lake, on the beach at a lake, in space)." .

pafh:Outcome a owl:Class ;
    rdfs:label "Outcome" ;
    iao:IAO_0000115 "A result or effect that is realized as the output of a physical activity process, such as improved health, fitness, or well-being."^^xsd:string ;
    rdfs:comment "Outcomes are typically modeled as outputs of processes (see ro:has_output)." .

pafh:hasAgent a owl:ObjectProperty ;
    rdfs:label "has agent" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:Agent ;
    rdfs:comment "Relates a physical activity to the agent(s) who perform or participate in it." .

pafh:hasSetting a owl:ObjectProperty ;
    rdfs:label "has setting" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:Setting ;
    rdfs:comment "Relates a physical activity to the setting in which it occurs." .

pafh:hasOutcome a owl:ObjectProperty ;
    rdfs:label "has outcome" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:Outcome ;
    rdfs:comment "Relates a physical activity to its outcomes or effects." .

