    skos:closeMatch <http://purl.obolibrary.org/obo/OGMS_0000098> ;
    skos:closeMatch <http://purl.bioontology.org/ontology/SNOMEDCT/161901003> .

pafh:EmotionalHealth a owl:Class ;
    rdfs:label "Emotional Health" ;
    rdfs:subClassOf pafh:MentalHealth ;
    iao:IAO_0000115 "The capacity to experience, regulate, and express emotions in ways that promote well-being. [WHO 2018, PMID:15003161]"^^xsd:string .

pafh:CognitiveHealth a owl:Class ;
    rdfs:label "Cognitive Health" ;
    rdfs:subClassOf pafh:MentalHealth ;
    iao:IAO_0000115 "The ability to think, learn, and remember, encompassing memory, attention, and executive function. [WHO 2018, PMID:15003161]"^^xsd:string .

pafh:SocialWellBeing a owl:Class ;
    rdfs:label "Social Well-Being" ;
    rdfs:subClassOf pafh:MentalHealth ;
    iao:IAO_0000115 "The quality of social relationships and support that contribute to overall mental health. [WHO 2018, PMID:15003161]"^^xsd:string .

pafh:PopulationHealth a owl:Class ;
    rdfs:label "Population Health" ;
    rdfs:subClassOf pafh:Health ;
