# --- MODERATORS BRANCH ---

pafh:Moderator a owl:Class ;
    rdfs:label "Moderator" ;
    rdfs:subClassOf bfo:DependentContinuant ;
    iao:IAO_0000115 "A dependent continuant that is a variable or factor modifying the strength or direction of the effect of a physical activity process on health, fitness, or functional outcomes. Moderators are not processes themselves, but qualities, roles, or conditions that influence causal relationships. [PMID:17145995; PMID:20401789; PMID:29261878; BFO]"^^xsd:string ;
    dcterms:source "PMID:17145995; PMID:20401789; PMID:29261878; BFO" .

pafh:DemographicModerator a owl:Class ;
    rdfs:label "Demographic Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in a population, related to characteristics such as age, sex, ethnicity, or socioeconomic status, and which modifies the effect of physical activity on outcomes. [PMID:29261878; PMID:3597653; PMID:9215334; BFO]"^^xsd:string ;
    dcterms:source "PMID:29261878; PMID:3597653; PMID:9215334; BFO" .

pafh:GeneticModerator a owl:Class ;
    rdfs:label "Genetic Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in an organism, related to genetic variation or heritable traits, and which modifies the effect of physical activity on outcomes. [PMID:17956337; BFO]"^^xsd:string ;
    dcterms:source "PMID:17956337; BFO" .

pafh:BehavioralModerator a owl:Class ;
    rdfs:label "Behavioral Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in a person or group, related to behaviors such as diet, sleep, or substance use, and which modifies the effect of physical activity on outcomes. [PMID:20401789; PMID:29261878; BFO]"^^xsd:string ;
    dcterms:source "PMID:20401789; PMID:29261878; BFO" .

pafh:PsychosocialModerator a owl:Class ;
    rdfs:label "Psychosocial Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in a person or group, related to psychological or social factors—such as motivation, social support, or mental health—that modifies the effect of physical activity on outcomes. [PMID:20401789; PMID:29261878; BFO]"^^xsd:string ;
    dcterms:source "PMID:20401789; PMID:29261878; BFO" .

pafh:EnvironmentalModerator a owl:Class ;
    rdfs:label "Environmental Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in the environment, related to physical, built, or natural environmental features (e.g., walkability, climate, access to facilities) that modifies the effect of physical activity on outcomes. [PMID:20401789; PMID:20064237; BFO]"^^xsd:string ;
    dcterms:source "PMID:20401789; PMID:20064237; BFO" .

pafh:ClinicalModerator a owl:Class ;
    rdfs:label "Clinical Moderator" ;
    rdfs:subClassOf pafh:Moderator ;
    iao:IAO_0000115 "A moderator that is a dependent continuant inhering in a person, related to medical conditions, treatments, or clinical characteristics (e.g., comorbidities, medication use) that modifies the effect of physical activity on outcomes. [PMID:11581152; PMID:29261878; BFO]"^^xsd:string ;
    dcterms:source "PMID:11581152; PMID:29261878; BFO" .

pafh:hasModerator a owl:ObjectProperty ;
    rdfs:label "has moderator" ;
    rdfs:domain pafh:PhysicalActivity ;
    rdfs:range pafh:Moderator ;
    rdfs:comment "Relates a physical activity to moderators (demographic, genetic, behavioral, psychosocial, environmental, or clinical) that influence its performance or outcomes." .

# --- END MODERATORS BRANCH ---

