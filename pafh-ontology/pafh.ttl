pafh:CirculatorySystemHealth a owl:Class ;
    rdfs:label "Circulatory System Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the circulatory system of an organism to transport blood, nutrients, gases, and wastes, realized by the function of blood vessels and the heart. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:ImmuneHealth a owl:Class ;
    rdfs:label "Immune Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
    iao:IAO_0000115 "A disposition of the immune system of an organism to defend against pathogens and disease, realized by immune responses and surveillance. [MedlinePlus, NIH, BFO]"^^xsd:string .

pafh:DigestiveSystemHealth a owl:Class ;
    rdfs:label "Digestive System Health" ;
    rdfs:subClassOf pafh:PhysiologicalHealth ;
