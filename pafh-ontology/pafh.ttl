pafh:PublicHealthIntervention a owl:Class ;
    rdfs:label "Public Health Intervention" ;
    rdfs:subClassOf pafh:PopulationHealth ;
    iao:IAO_0000115 "Programs, policies, or actions designed to improve the health of populations and reduce health disparities. [CDC 2023, PMID:12682478]"^^xsd:string .

